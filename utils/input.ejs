<?xml version="1.0" encoding="UTF-8"?>
<document>
  <align mode="left">
    <text-line size="1:0"><%= name %>   Masa <%= masa %></text-line>
  </align>

  <line-feed/>
  <line-feed/>

  <!-- Start of loop -->
  <% for (let product of products) { %>
    <align mode="right">
      <text-line size="1:0"><%= product.name%> X <%=product.qty%></text-line>
    </align>
    <line-feed/>
    <% } %>
  <!-- End of loop -->

  <paper-cut></paper-cut>
</document>
